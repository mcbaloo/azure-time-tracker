{
  "manifestVersion": 1,
  "id": "azure-devops-time-tracker-extension",
  "version": "2.0.8",
  "name": "Time Tracker Logger",
  "public": true,
  "description": "Track and log time spent on work items and generate detailed reports",
  "publisher": "mcbaloo-apps",
  "links": {
        "support": {
            "uri": "https://github.com/mcbaloo/azure-time-tracker/issues"
        },
        "repository": {
            "uri": "https://github.com/mcbaloo/azure-time-tracker"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mcbaloo/azure-time-tracker"
    },
  "categories": ["Azure Boards"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "scopes": [
    "vso.work",
    "vso.work_write"
  ],
  "contributions": [
    {
      "id": "time-summary-work-item-form",
      "type": "ms.vss-work-web.work-item-form-group",
      "description": "Shows time logged on work item",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Time Logged",
        "uri": "dist/time-summary/time-summary.html",
        "height": 50
      }
    },
    {
      "id": "time-entry-work-item-form",
      "type": "ms.vss-work-web.work-item-form-page",
      "description": "Add time entries to work items",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Time Tracking",
        "uri": "dist/time-entry/time-entry.html"
      }
    },
    {
      "id": "time-tracking-hub",
      "type": "ms.vss-web.hub",
      "description": "Time tracking reports and analytics",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Time Reports",
        "order": 100,
        "uri": "dist/reports/reports.html",
        "icon": "asset://dist/images/report-icon.png"
      }
    }
  ]
}
